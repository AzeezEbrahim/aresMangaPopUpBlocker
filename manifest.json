{
    "manifest_version": 3,
    "name": "aresMangaAdsRemoval",
    "description": "Removes ads from https://aresmanga.com/",
    "version": "1.0",
    "icons": {
        "16": "/images/ghost-16.png",
        "32": "/images/ghost-32.png",
        "128": "/images/ghost-128.png"
    },
    "content_scripts": [
        {
            "matches": ["http://aresmanga.com/*", "https://aresmanga.com/*"],
            "js": ["remove.js"]
        }
    ]


}